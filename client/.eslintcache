} from '../utils/helpers';\r\nimport spinner from '../assets/spinner.gif';\r\n\r\nfunction gameStore() {\r\n  const [state, dispatch] = useStoreContext();\r\n  const { id } = useParams();\r\n\r\n  const [currentGame, setCurrentGame] = useState({});\r\n\r\n  const { loading, data } = useQuery(QUERY_GAMES);\r\n\r\n  const { games, cart } = state;\r\n\r\n  useEffect(() => {\r\n    // already in global store\r\n    if (games.length) {\r\n      setCurrentGame(games.find((game) => game._id === id));\r\n    }\r\n    // retrieved from server\r\n    else if (data) {\r\n      dispatch({\r\n        type: UPDATE_GAME,\r\n        games: data.games,\r\n      });\r\n\r\n      data.games.forEach((game) => {\r\n        idbPromise('games', 'put', game);\r\n      });\r\n    }\r\n    // get cache from idb\r\n    else if (!loading) {\r\n      idbPromise('games', 'get').then((indexedGames) => {\r\n        dispatch({\r\n          type: UPDATE_GAME,\r\n          games: indexedGames,\r\n        });\r\n      });\r\n    }\r\n  }, [games, data, loading, dispatch, id]);\r\n\r\n  const addToCart = () => {\r\n    const itemInCart = cart.find((cartItem) => cartItem._id === id);\r\n    if (itemInCart) {\r\n      dispatch({\r\n        type: UPDATE_CART_QUANTITY,\r\n        _id: id,\r\n        purchaseQuantity: parseInt(itemInCart.purchaseQuantity) + 1,\r\n      });\r\n      idbPromise('cart', 'put', {\r\n        ...itemInCart,\r\n        purchaseQuantity: parseInt(itemInCart.purchaseQuantity) + 1,\r\n      });\r\n    } else {\r\n      dispatch({\r\n        type: ADD_TO_CART,\r\n        game: { ...currentGame, purchaseQuantity: 1 },\r\n      });\r\n      idbPromise('cart', 'put', { ...currentGame, purchaseQuantity: 1 });\r\n    }\r\n  };\r\n\r\n  const removeFromCart = () => {\r\n    dispatch({\r\n      type: REMOVE_FROM_CART,\r\n      _id: currentGame._id,\r\n    });\r\n\r\n    idbPromise('cart', 'delete', { ...setCurrentGame });\r\n  };\r\n\r\n  return (\r\n    <>\r\n      {currentGame && cart ? (\r\n        <div className=\"container my-1\">\r\n          <Link to=\"/\">← Back to Games</Link>\r\n\r\n          <h2>{currentGame.name}</h2>\r\n\r\n          <p>{currentGame.description}</p>\r\n\r\n          <p>\r\n            <strong>Price:</strong>${currentGame.price}{' '}\r\n            <button onClick={addToCart}>Add to Cart</button>\r\n            <button\r\n              disabled={!cart.find((p) => p._id === currentGame._id)}\r\n              onClick={removeFromCart}\r\n            >\r\n              Remove from Cart\r\n            </button>\r\n          </p>\r\n\r\n          <img\r\n            src={`/images/${currentGame.image}`}\r\n            alt={currentGame.name}\r\n          />\r\n        </div>\r\n      ) : null}\r\n      {loading ? <img src={spinner} alt=\"loading\" /> : null}\r\n      <Cart />\r\n    </>\r\n  );\r\n};\r\n\r\nexport default gameStore;\r\n","C:\\Users\\thoma\\Desktop\\Gamers-Wanted\\client\\src\\pages\\Login.js",["104"],"import React, { useState } from 'react';\r\nimport { useMutation } from '@apollo/client';\r\nimport { Link } from 'react-router-dom';\r\nimport { LOGIN } from '../utils/mutations';\r\nimport Auth from '../utils/auth';\r\n\r\nfunction Login(props) {\r\n  const [formState, setFormState] = useState({ email: '', password: '' });\r\n  const [login, { error }] = useMutation(LOGIN);\r\n\r\n  const handleFormSubmit = async (evt) => {\r\n    evt.preventDefault();\r\n    try {\r\n      const mutationResponse = await login({\r\n        variables: { email: formState.email, password: formState.password },\r\n      });\r\n      const token = mutationResponse.data.login.token;\r\n      Auth.login(token);\r\n    } catch (e) {\r\n      console.log(e);\r\n    }\r\n  };\r\n\r\n  const handleChange = (evt) => {\r\n    const { name, value } = event.target;\r\n    setFormState({\r\n      ...formState,\r\n      [name]: value,\r\n    });\r\n  };\r\n\r\n  return (\r\n    <div className=\"container my-1\">\r\n      <Link to=\"/signup\">← Go to Signup</Link>\r\n\r\n      <h2>Login</h2>\r\n      <form onSubmit={handleFormSubmit}>\r\n        <div className=\"flex-row space-between my-2\">\r\n          <label htmlFor=\"email\">Email address:</label>\r\n          <input\r\n            placeholder=\"your-email@test.com\"\r\n            name=\"email\"\r\n            type=\"email\"\r\n            id=\"email\"\r\n            onChange={handleChange}\r\n          />\r\n        </div>\r\n        <div className=\"flex-row space-between my-2\">\r\n          <label htmlFor=\"pwd\">Password:</label>\r\n          <input\r\n            placeholder=\"******\"\r\n            name=\"password\"\r\n            type=\"password\"\r\n            id=\"pwd\"\r\n            onChange={handleChange}\r\n          />\r\n        </div>\r\n        {error ? (\r\n          <div>\r\n            <p className=\"error-text\">Swing and a miss, the information is incorrect</p>\r\n          </div>\r\n        ) : null}\r\n        <div className=\"flex-row flex-end\">\r\n          <button type=\"submit\">Submit</button>\r\n        </div>\r\n      </form>\r\n    </div>\r\n  );\r\n}\r\n\r\nexport default Login;","C:\\Users\\thoma\\Desktop\\Gamers-Wanted\\client\\src\\pages\\NotHere.js",[],"C:\\Users\\thoma\\Desktop\\Gamers-Wanted\\client\\src\\pages\\Signup.js",[],"C:\\Users\\thoma\\Desktop\\Gamers-Wanted\\client\\src\\pages\\GameOrder.js",[],"C:\\Users\\thoma\\Desktop\\Gamers-Wanted\\client\\src\\pages\\PrevOrders.js",[],"C:\\Users\\thoma\\Desktop\\Gamers-Wanted\\client\\src\\utils\\GlobalState.js",[],"C:\\Users\\thoma\\Desktop\\Gamers-Wanted\\client\\src\\components\\Nav\\index.js",[],"C:\\Users\\thoma\\Desktop\\Gamers-Wanted\\client\\src\\utils\\queries.js",[],"C:\\Users\\thoma\\Desktop\\Gamers-Wanted\\client\\src\\utils\\helpers.js",[],"C:\\Users\\thoma\\Desktop\\Gamers-Wanted\\client\\src\\utils\\actions.js",[],"C:\\Users\\thoma\\Desktop\\Gamers-Wanted\\client\\src\\utils\\mutations.js",[],"C:\\Users\\thoma\\Desktop\\Gamers-Wanted\\client\\src\\u